{
  "name": "react-build-browser",
  "version": "0.1.0",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "1.0.0",
    "react-scripts": "2.1.3",
    "serve": "^10.1.2",
    "webpack": "3.8.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "react": ">=16.7.0",
    "react-dom": ">=16.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve:dist": "cd dist && ../node_modules/serve/bin/serve.js",
    "serve": "npm run serve:dist",
    "build:browser": "rm -rf ./dist && NODE_ENV=production webpack --config ./config/webpack.config.browser.js && cp ./public/index.browser.html ./dist/index.html",
    "build": "npm run build:browser",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
